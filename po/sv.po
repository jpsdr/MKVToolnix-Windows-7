#: src/merge/mkvmerge.cpp:96
#, no-c-format
msgid " Chapter handling:\n"
msgstr " Kapitelhantering:\n"

#: src/merge/mkvmerge.cpp:97
#, no-c-format
msgid "  --chapters <file>        Read chapter information from the file.\n"
msgstr "  --chapters <file> Läs kapitelinformation från filen.\n"

#: src/merge/mkvmerge.cpp:98
#, no-c-format
msgid "  --chapter-language <lng> Set the 'language' element in chapter entries.\n"
msgstr "  --chapter-language <lng> Ställ in elementet \"language\" i kapitelposter.\n"

#: src/merge/mkvmerge.cpp:99
#, no-c-format
msgid "  --chapter-charset <cset> Charset for a simple chapter file.\n"
msgstr "  --chapter-charset <cset> Teckensnitt för en enkel kapitelfil.\n"

#: src/merge/mkvmerge.cpp:100
#, no-c-format
msgid ""
"  --chapter-sync <d[,o[/p]]>\n"
"                           Synchronize, adjust the chapters's timestamps\n"
"                           by 'd' ms.\n"
"                           'o/p': Adjust the timestamps by multiplying with\n"
"                           'o/p' to fix linear drifts. 'p' defaults to\n"
"                           1 if omitted. Both 'o' and 'p' can be\n"
"                           floating point numbers.\n"
msgstr ""
"  --chapter-sync <d[,o[/p]]>\n"
"                           Synkronisera, justera kapitlens tidsstämplar\n"
"                           med 'd' ms.\n"
"                           'o/p': Justera tidsstämplarna genom att multiplicera med\n"
"                           'o/p' för att korrigera linjära förskjutningar. 'p' är standardvärdet\n"
"                           1 om det utelämnas. Både 'o' och 'p' kan vara\n"
"                           flyttal.\n"

#: src/merge/mkvmerge.cpp:107
#, no-c-format
msgid ""
"  --cue-chapter-name-format <format>\n"
"                           Pattern for the conversion from cue sheet\n"
"                           entries to chapter names.\n"
msgstr ""
"  --cue-kapitel-namn-format <format>\n"
"                           Mönster för konvertering från kölappar\n"
"                           poster till kapitelnamn.\n"

#: src/merge/mkvmerge.cpp:110
#, no-c-format
msgid ""
"  --default-language <lng> Use this language for all tracks unless\n"
"                           overridden with the --language option.\n"
msgstr ""
"  --default-language <lng> Använd detta språk för alla spår om inte\n"
"                           om det inte åsidosätts med alternativet --language.\n"

#: src/merge/mkvmerge.cpp:112
#, no-c-format
msgid ""
"  --generate-chapters <mode>\n"
"                           Automatically generate chapters according to\n"
"                           the mode ('when-appending' or 'interval:<duration>').\n"
msgstr ""
"  --generate-chapters <mode>\n"
"                           Generera automatiskt kapitel enligt\n"
"                           läget (\"när du lägger till\" eller \"intervall:<duration>\").\n"

#: src/merge/mkvmerge.cpp:115
#, no-c-format
msgid ""
"  --generate-chapters-name-template <template>\n"
"                           Template for newly generated chapter names\n"
"                           (default: 'Chapter <NUM:2>').\n"
msgstr ""
"  --generate-chapters-name-template <template>\n"
"                           Mall för nygenererade kapitelnamn\n"
"                           (standard: \"Kapitel <NUM:2>\").\n"

#: src/merge/mkvmerge.cpp:119
#, no-c-format
msgid " Segment info handling:\n"
msgstr " Hantering av segmentinformation:\n"

#: src/merge/mkvmerge.cpp:120
#, no-c-format
msgid "  --segmentinfo <file>     Read segment information from the file.\n"
msgstr "  --segmentinfo <file> Läs segmentinformation från filen.\n"

#: src/merge/mkvmerge.cpp:121
#, no-c-format
msgid ""
"  --segment-uid <SID1,[SID2...]>\n"
"                           Set the segment UIDs to SID1, SID2 etc.\n"
msgstr ""
"  --segment-uid <SID1,[SID2...]> -segment-uid\n"
"                           Ställ in segmentens UID:er till SID1, SID2 etc.\n"

#: src/merge/mkvmerge.cpp:124
#, no-c-format
msgid " General output control (advanced global options):\n"
msgstr " Allmän styrning av utdata (avancerade globala alternativ):\n"

#: src/merge/mkvmerge.cpp:125
#, no-c-format
msgid ""
"  --track-order <FileID1:TID1,FileID2:TID2,FileID3:TID3,...>\n"
"                           A comma separated list of both file IDs\n"
"                           and track IDs that controls the order of the\n"
"                           tracks in the destination file. If not given,\n"
"                           the tracks will sorted by type (video, audio,\n"
"                           subtitles, others).\n"
msgstr ""
"  --track-order <FileID1:TID1,FileID2:TID2,FileID3:TID3,...> -track-order\n"
"                           En kommaseparerad lista med både fil-ID:n\n"
"                           och spår-ID:n som styr ordningen på spåren i\n"
"                           spåren i målfilen. Om den inte anges,\n"
"                           kommer spåren att sorteras efter typ (video, ljud,\n"
"                           undertexter, andra).\n"

#: src/merge/mkvmerge.cpp:131
#, no-c-format
msgid ""
"  --cluster-length <n[ms]> Put at most n data blocks into each cluster.\n"
"                           If the number is postfixed with 'ms' then\n"
"                           put at most n milliseconds of data into each\n"
"                           cluster.\n"
msgstr ""
"  --cluster-length <n[ms]> Lägg in högst n datablock i varje kluster.\n"
"                           Om antalet är postfixerat med 'ms' så\n"
"                           högst n millisekunder av data i varje kluster\n"
"                           kluster.\n"

#: src/merge/mkvmerge.cpp:135
#, no-c-format
msgid "  --clusters-in-meta-seek  Write meta seek data for clusters.\n"
msgstr "  --clusters-in-meta-seek  Skriva metasökdata för kluster.\n"

#: src/merge/mkvmerge.cpp:136
#, no-c-format
msgid "  --timestamp-scale <n>    Force the timestamp scale factor to n.\n"
msgstr "  --timestamp-scale <n> Ställ in tidsstämpelns skalfaktor på n.\n"

#: src/merge/mkvmerge.cpp:137
#, no-c-format
msgid "  --enable-durations       Enable block durations for all blocks.\n"
msgstr "  --enable-durations Aktivera blockvaraktighet för alla block.\n"

#: src/merge/mkvmerge.cpp:138
#, no-c-format
msgid "  --no-cues                Do not write the cue data (the index).\n"
msgstr "  --no-cues Skriv inte cue-data (indexet).\n"

#: src/merge/mkvmerge.cpp:139
#, no-c-format
msgid ""
"  --no-date                Do not write the 'date' field in the segment\n"
"                           information headers.\n"
msgstr ""
"  --no-date Skriv inte fältet \"date\" i segmentinformationens\n"
"                           informationsrubrikerna.\n"

#: src/merge/mkvmerge.cpp:141
#, no-c-format
msgid ""
"  --date <timestamp>       Set the 'date' field (UTC or with offset,\n"
"                           e.g. 2017-03-28T17:28:00Z).\n"
msgstr ""

#: src/merge/mkvmerge.cpp:143
#, no-c-format
msgid "  --disable-lacing         Do not use lacing.\n"
msgstr "  --disable-lacing Använd inte lacing.\n"

#: src/merge/mkvmerge.cpp:144
#, no-c-format
msgid ""
"  --disable-track-statistics-tags\n"
"                           Do not write tags with track statistics.\n"
msgstr ""
"  --disable-track-statistics-tags\n"
"                           Skriv inte taggar med spårstatistik.\n"

#: src/merge/mkvmerge.cpp:146
#, no-c-format
msgid ""
"  --disable-language-ietf  Do not write IETF BCP 47 language elements in\n"
"                           track headers, chapters and tags.\n"
msgstr ""
"  --disable-language-ietf Skriv inte IETF BCP 47-språkelement i\n"
"                           spårrubriker, kapitel och taggar.\n"

#: src/merge/mkvmerge.cpp:148
#, no-c-format
msgid ""
"  --normalize-language-ietf <canonical|extlang|off>\n"
"                           Normalize all IETF BCP 47 language tags to either\n"
"                           their canonical or their extended language subtags\n"
"                           form or not at all (default: canonical form).\n"
msgstr ""
"  --normalize-language-ietf <canonical|extlang|off> (normalisera språk-ietf)\n"
"                           Normalisera alla IETF BCP 47-språktaggar till antingen\n"
"                           deras kanoniska eller deras utökade språkundertaggar\n"
"                           form eller inte alls (standard: kanonisk form).\n"

#: src/merge/mkvmerge.cpp:152
#, no-c-format
msgid ""
"  --stop-after-video-ends  Stops processing after the primary video track ends,\n"
"                           discarding any remaining packets of other tracks.\n"
msgstr ""
"  --stop-after-video-ends Stoppar bearbetningen när det primära videospåret är slut,\n"
"                           och kasserar eventuella återstående paket från andra spår.\n"

#: src/merge/mkvmerge.cpp:155
#, no-c-format
msgid " File splitting, linking, appending and concatenating (more global options):\n"
msgstr " Fildelning, länkning, appendiering och konkatenering (fler globala alternativ):\n"

#: src/merge/mkvmerge.cpp:156
#, no-c-format
msgid ""
"  --split <d[K,M,G]|HH:MM:SS|s>\n"
"                           Create a new file after d bytes (KB, MB, GB)\n"
"                           or after a specific time.\n"
msgstr ""
"  --split <d[K,M,G]|HH:MM:SS|s>\n"
"                           Skapa en ny fil efter d byte (KB, MB, GB)\n"
"                           eller efter en viss tid.\n"

#: src/merge/mkvmerge.cpp:159
#, no-c-format
msgid ""
"  --split timestamps:A[,B...]\n"
"                           Create a new file after each timestamp A, B\n"
"                           etc.\n"
msgstr ""
"  --split tidsstämplar:A[,B...]\n"
"                           Skapa en ny fil efter varje tidsstämpel A, B\n"
"                           etc.\n"

#: src/merge/mkvmerge.cpp:162
#, no-c-format
msgid ""
"  --split parts:start1-end1[,[+]start2-end2,...]\n"
"                           Keep ranges of timestamps start-end, either in\n"
"                           separate files or append to previous range's file\n"
"                           if prefixed with '+'.\n"
msgstr ""
"  --split parts:start1-slut1[,[+]start2-slut2,...]\n"
"                           Behåll intervall av tidsstämplar start-slut, antingen i\n"
"                           separata filer eller lägg till i föregående intervalls fil\n"
"                           om den har prefixet \"+\".\n"

#: src/merge/mkvmerge.cpp:166
#, no-c-format
msgid ""
"  --split parts-frames:start1-end1[,[+]start2-end2,...]\n"
"                           Same as 'parts:', but 'startN'/'endN' are frame/\n"
"                           field numbers instead of timestamps.\n"
msgstr ""
"  --split parts-frames:start1-slut1[,[+]start2-slut2,...]\n"
"                           Samma som \"parts:\", men \"startN\"/\"endN\" är ram/\n"
"                           fältnummer istället för tidsstämplar.\n"

#: src/merge/mkvmerge.cpp:169
#, no-c-format
msgid ""
"  --split frames:A[,B...]\n"
"                           Create a new file after each frame/field A, B\n"
"                           etc.\n"
msgstr ""
"  --split frames:A[,B...]\n"
"                           Skapa en ny fil efter varje bildruta/fält A, B\n"
"                           etc.\n"

#: src/merge/mkvmerge.cpp:172
#, no-c-format
msgid ""
"  --split chapters:all|A[,B...]\n"
"                           Create a new file before each chapter (with 'all')\n"
"                           or before chapter numbers A, B etc.\n"
msgstr ""
"  --split kapitel:all|A[,B...]\n"
"                           Skapa en ny fil före varje kapitel (med 'all')\n"
"                           eller före kapitelnumren A, B etc.\n"

#: src/merge/mkvmerge.cpp:175
#, no-c-format
msgid "  --split-max-files <n>    Create at most n files.\n"
msgstr "  --split-max-files <n> Skapa högst n filer.\n"

#: src/merge/mkvmerge.cpp:176
#, no-c-format
msgid "  --link                   Link splitted files.\n"
msgstr "  --link Länka delade filer.\n"

#: src/merge/mkvmerge.cpp:177
#, no-c-format
msgid "  --link-to-previous <SID> Link the first file to the given SID.\n"
msgstr "  --link-to-previous <SID> Länka den första filen till den angivna SID.\n"

#: src/merge/mkvmerge.cpp:178
#, no-c-format
msgid "  --link-to-next <SID>     Link the last file to the given SID.\n"
msgstr "  --link-to-next <SID> Länka den sista filen till den angivna SID.\n"

#: src/merge/mkvmerge.cpp:179
#, no-c-format
msgid ""
"  --append-to <SFID1:STID1:DFID1:DTID1,SFID2:STID2:DFID2:DTID2,...>\n"
"                           A comma separated list of file and track IDs\n"
"                           that controls which track of a file is\n"
"                           appended to another track of the preceding\n"
"                           file.\n"
msgstr ""
"  --append-to <SFID1:STID1:DFID1:DTID1,SFID2:STID2:DFID2:DTID2,...>\n"
"                           En kommaseparerad lista med fil- och spår-ID\n"
"                           som styr vilket spår i en fil som ska\n"
"                           till ett annat spår i den föregående filen\n"
"                           föregående filen.\n"

#: src/merge/mkvmerge.cpp:184
#, no-c-format
msgid ""
"  --append-mode <file|track>\n"
"                           Selects how mkvmerge calculates timestamps when\n"
"                           appending files.\n"
msgstr ""
"  --append-läge <fil|spår>\n"
"                           Väljer hur mkvmerge beräknar tidsstämplar när\n"
"                           när filer läggs till.\n"

#: src/merge/mkvmerge.cpp:187
#, no-c-format
msgid "  <file1> + <file2>        Append file2 to file1.\n"
msgstr "  <fil1> + <fil2> Lägg till fil2 i fil1.\n"

#: src/merge/mkvmerge.cpp:188
#, no-c-format
msgid "  <file1> +<file2>         Same as \"<file1> + <file2>\".\n"
msgstr "  <fil1> +<fil2> Samma som \"<fil1> + <fil2>\".\n"

#: src/merge/mkvmerge.cpp:189
#, no-c-format
msgid "  [ <file1> <file2> ]      Same as \"<file1> + <file2>\".\n"
msgstr "  [ <fil1> <fil2> ] Samma som \"<fil1> + <fil2>\".\n"

#: src/merge/mkvmerge.cpp:190
#, no-c-format
msgid ""
"  = <file>                 Don't look for and concatenate files with the same\n"
"                           base name but with a different trailing number.\n"
msgstr ""
"  = <fil> Leta inte efter och sammanlänka filer med samma\n"
"                           basnamn men med ett annat efterföljande nummer.\n"

#: src/merge/mkvmerge.cpp:192
#, no-c-format
msgid "  =<file>                  Same as \"= <file>\".\n"
msgstr "  =<fil> Samma som \"= <fil>\".\n"

#: src/merge/mkvmerge.cpp:193
#, no-c-format
msgid ""
"  ( <file1> <file2> )      Treat file1 and file2 as if they were concatenated\n"
"                           into a single big file.\n"
msgstr ""
"  ( <fil1> <fil2> ) Behandla fil1 och fil2 som om de var sammankopplade\n"
"                           till en enda stor fil.\n"

#: src/merge/mkvmerge.cpp:196
#, no-c-format
msgid " Attachment support (more global options):\n"
msgstr " Stöd för fastsättning (fler globala alternativ):\n"

#: src/merge/mkvmerge.cpp:197
#, no-c-format
msgid ""
"  --attachment-description <desc>\n"
"                           Description for the following attachment.\n"
msgstr ""
"  --beskrivning av bifogad fil <desc>\n"
"                           Beskrivning för följande bilaga.\n"

#: src/merge/mkvmerge.cpp:199
#, no-c-format
msgid ""
"  --attachment-mime-type <mime type>\n"
"                           Mime type for the following attachment.\n"
msgstr ""
"  --attachment-mime-typ <mime type>\n"
"                           Mime-typ för följande bilaga.\n"

#: src/merge/mkvmerge.cpp:201
#, no-c-format
msgid ""
"  --attachment-name <name> The name should be stored for the \n"
"                           following attachment.\n"
msgstr ""
"  --attachment-name <name> Namnet som ska sparas för följande \n"
"                           följande bilaga.\n"

#: src/merge/mkvmerge.cpp:203
#, no-c-format
msgid ""
"  --attach-file <file>     Creates a file attachment inside the\n"
"                           Matroska file.\n"
msgstr ""
"  --attach-file <file> Skapar en bifogad fil i Matroska-filen\n"
"                           Matroska-filen.\n"

#: src/merge/mkvmerge.cpp:205
#, no-c-format
msgid ""
"  --attach-file-once <file>\n"
"                           Creates a file attachment inside the\n"
"                           first Matroska file written.\n"
msgstr ""
"  --attach-fil-en-gång <file>\n"
"                           Skapar en filbilaga inuti den\n"
"                           första Matroska-filen som skrivs.\n"

#: src/merge/mkvmerge.cpp:208
#, no-c-format
msgid ""
"  --enable-legacy-font-mime-types\n"
"                           Use legacy font MIME types when adding new\n"
"                           attachments as well as for existing ones.\n"
msgstr ""
"  --enable-legacy-teckensnitt-mime-typer\n"
"                           Använd MIME-typer för äldre teckensnitt när du lägger till nya\n"
"                           bilagor samt för befintliga bilagor.\n"

#: src/merge/mkvmerge.cpp:212
#, no-c-format
msgid " Options for each source file:\n"
msgstr " Alternativ för varje källfil:\n"

#: src/merge/mkvmerge.cpp:213
#, no-c-format
msgid ""
"  -a, --audio-tracks <n,m,...>\n"
"                           Copy audio tracks n,m etc. Default: copy all\n"
"                           audio tracks.\n"
msgstr ""
"  -a, --audio-tracks <n,m,...>\n"
"                           Kopiera ljudspår n,m etc. Standard: kopiera alla\n"
"                           ljudspår.\n"

#: src/merge/mkvmerge.cpp:216
#, no-c-format
msgid "  -A, --no-audio           Don't copy any audio track from this file.\n"
msgstr "  -A, --no-audio Kopiera inte något ljudspår från den här filen.\n"

#: src/merge/mkvmerge.cpp:217
#, no-c-format
msgid ""
"  -d, --video-tracks <n,m,...>\n"
"                           Copy video tracks n,m etc. Default: copy all\n"
"                           video tracks.\n"
msgstr ""
"  -d, --video-tracks <n,m,...>\n"
"                           Kopiera videospår n,m etc. Standard: kopiera alla\n"
"                           videospår.\n"

#: src/merge/mkvmerge.cpp:220
#, no-c-format
msgid "  -D, --no-video           Don't copy any video track from this file.\n"
msgstr "  -D, --no-video Kopiera inte något videospår från den här filen.\n"

#: src/merge/mkvmerge.cpp:221
#, no-c-format
msgid ""
"  -s, --subtitle-tracks <n,m,...>\n"
"                           Copy subtitle tracks n,m etc. Default: copy\n"
"                           all subtitle tracks.\n"
msgstr ""
"  -s, --subtitle-tracks <n,m,...> (undertextspår)\n"
"                           Kopiera undertextspår n,m etc. Standard: kopiera\n"
"                           alla undertextspår.\n"

#: src/merge/mkvmerge.cpp:224
#, no-c-format
msgid "  -S, --no-subtitles       Don't copy any subtitle track from this file.\n"
msgstr "  -S, --no-subtitles Kopiera inte något undertextspår från den här filen.\n"

#: src/merge/mkvmerge.cpp:225
#, no-c-format
msgid ""
"  -b, --button-tracks <n,m,...>\n"
"                           Copy buttons tracks n,m etc. Default: copy\n"
"                           all buttons tracks.\n"
msgstr ""
"  -b, --button-tracks <n,m,...>\n"
"                           Kopiera knappspår n,m etc. Standard: kopiera\n"
"                           alla knappspår.\n"

#: src/merge/mkvmerge.cpp:228
#, no-c-format
msgid "  -B, --no-buttons         Don't copy any buttons track from this file.\n"
msgstr "  -B, --no-buttons Kopiera inte något knappspår från den här filen.\n"

#: src/merge/mkvmerge.cpp:229
#, no-c-format
msgid ""
"  -m, --attachments <n[:all|first],m[:all|first],...>\n"
"                           Copy the attachments with the IDs n, m etc. to\n"
"                           all or only the first destination file. Default:\n"
"                           copy all attachments to all destination files.\n"
msgstr ""
"  -m, --attachments <n[:alla|först],m[:alla|först],...>\n"
"                           Kopiera bilagorna med ID:n n, m etc. till alla eller\n"
"                           till alla eller endast den första målfilen. Standardinställning:\n"
"                           kopiera alla bilagor till alla målfiler.\n"

#: src/merge/mkvmerge.cpp:233
#, no-c-format
msgid "  -M, --no-attachments     Don't copy attachments from a source file.\n"
msgstr "  -M, --no-attachments Kopierar inte bilagor från en källfil.\n"

#: src/merge/mkvmerge.cpp:234
#, no-c-format
msgid "  -t, --tags <TID:file>    Read tags for the track from an XML file.\n"
msgstr "  -t, --tags <TID:file> Läs in taggar för spåret från en XML-fil.\n"

#: src/merge/mkvmerge.cpp:235
#, no-c-format
msgid ""
"  --track-tags <n,m,...>   Copy the tags for tracks n,m etc. Default: copy\n"
"                           tags for all tracks.\n"
msgstr ""
"  --track-tags <n,m,...> Kopiera taggarna för spår n,m etc. Standard: kopiera\n"
"                           taggar för alla spår.\n"

#: src/merge/mkvmerge.cpp:237
#, no-c-format
msgid "  -T, --no-track-tags      Don't copy tags for tracks from the source file.\n"
msgstr "  -T, --no-track-tags Kopiera inte taggar för spår från källfilen.\n"

#: src/merge/mkvmerge.cpp:238
#, no-c-format
msgid "  --no-global-tags         Don't keep global tags from the source file.\n"
msgstr "  --no-global-tags Behåll inte globala taggar från källfilen.\n"

#: src/merge/mkvmerge.cpp:239
#, no-c-format
msgid "  --no-chapters            Don't keep chapters from the source file.\n"
msgstr "  --no-chapters Behåller inte kapitel från källfilen.\n"

#: src/merge/mkvmerge.cpp:240
#, no-c-format
msgid ""
"  --regenerate-track-uids  Generate new random track UIDs instead of keeping\n"
"                           existing ones.\n"
msgstr ""
"  --regenerate-track-uids Generera nya slumpmässiga UID:er för spår istället för att behålla\n"
"                           befintliga.\n"

#: src/merge/mkvmerge.cpp:242
#, no-c-format
msgid ""
"  -y, --sync <TID:d[,o[/p]]>\n"
"                           Synchronize, adjust the track's timestamps with\n"
"                           the id TID by 'd' ms.\n"
"                           'o/p': Adjust the timestamps by multiplying with\n"
"                           'o/p' to fix linear drifts. 'p' defaults to\n"
"                           1 if omitted. Both 'o' and 'p' can be\n"
"                           floating point numbers.\n"
msgstr ""
"  -y, --sync <TID:d[,o[/p]]>\n"
"                           Synkronisera, justera spårets tidsstämplar med\n"
"                           id TID med 'd' ms.\n"
"                           'o/p': Justera tidsstämplarna genom att multiplicera med\n"
"                           \"o/p\" för att korrigera linjära avvikelser. 'p' är standardvärdet\n"
"                           1 om det utelämnas. Både 'o' och 'p' kan vara\n"
"                           flyttal.\n"

#: src/merge/mkvmerge.cpp:249
#, no-c-format
msgid ""
"  --default-track-flag <TID[:bool]>\n"
"                           Sets the \"default track\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --default-track-flag <TID[:bool]>\n"
"                           Ställer in flaggan för \"standardspår\" för detta spår eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:252
#, no-c-format
msgid ""
"  --forced-display-flag <TID[:bool]>\n"
"                           Sets the \"forced display\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --forced-display-flag <TID[:bool]>\n"
"                           Ställer in flaggan \"tvingad visning\" för det här spåret eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:255
#, no-c-format
msgid ""
"  --track-enabled-flag <TID[:bool]>\n"
"                           Sets the \"track enabled\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --track-enabled-flag <TID[:bool]>\n"
"                           Ställer in flaggan \"spår aktiverat\" för det här spåret eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:258
#, no-c-format
msgid ""
"  --hearing-impaired-flag <TID[:bool]>\n"
"                           Sets the \"hearing impaired\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --hearing-impaired-flag <TID[:bool]>\n"
"                           Ställer in flaggan för \"hörselskadad\" för detta spår eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:261
#, no-c-format
msgid ""
"  --visual-impaired-flag <TID[:bool]>\n"
"                           Sets the \"visual impaired\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --visual-impaired-flag <TID[:bool]>\n"
"                           Ställer in flaggan för \"synskadad\" för detta spår eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:264
#, no-c-format
msgid ""
"  --text-descriptions-flag <TID[:bool]>\n"
"                           Sets the \"text descriptions\" flag for this track\n"
"                           or forces it not to be present if bool is 0.\n"
msgstr ""
"  --text-descriptions-flag <TID[:bool]>\n"
"                           Ställer in flaggan \"textbeskrivningar\" för det här spåret\n"
"                           eller tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:267
#, no-c-format
msgid ""
"  --original-flag <TID[:bool]>\n"
"                           Sets the \"original language\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --original-flag <TID[:bool]>\n"
"                           Ställer in flaggan för \"originalspråk\" för detta spår eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:270
#, no-c-format
msgid ""
"  --commentary-flag <TID[:bool]>\n"
"                           Sets the \"commentary\" flag for this track or\n"
"                           forces it not to be present if bool is 0.\n"
msgstr ""
"  --commentary-flag <TID[:bool]>\n"
"                           Ställer in \"kommentar\"-flaggan för det här spåret eller\n"
"                           tvingar den att inte vara närvarande om bool är 0.\n"

#: src/merge/mkvmerge.cpp:273
#, no-c-format
msgid "  --track-name <TID:name>  Sets the name for a track.\n"
msgstr "  --track-name <TID:name> Ställer in namnet på ett spår.\n"

#: src/merge/mkvmerge.cpp:274
#, no-c-format
msgid ""
"  --cues <TID:none|iframes|all>\n"
"                           Create cue (index) entries for this track:\n"
"                           None at all, only for I frames, for all.\n"
msgstr ""
"  --cues <TID:ingen|iframes|alla>\n"
"                           Skapa cue-poster (index) för det här spåret:\n"
"                           Ingen alls, endast för I frames, för alla.\n"

#: src/merge/mkvmerge.cpp:277
#, no-c-format
msgid ""
"  --language <TID:lang>    Sets the language for the track (IETF BCP 47/\n"
"                           RFC 5646 language tag).\n"
msgstr ""
"  --language <TID:lang> Ställer in språket för spåret (IETF BCP 47/\n"
"                           RFC 5646 språktagg).\n"

#: src/merge/mkvmerge.cpp:279
#, no-c-format
msgid "  --timestamps <TID:file>  Read the timestamps to be used from a file.\n"
msgstr "  --timestamps <TID:file> Läs in de tidsstämplar som ska användas från en fil.\n"

#: src/merge/mkvmerge.cpp:280
#, no-c-format
msgid ""
"  --default-duration <TID:Xs|ms|us|ns|fps>\n"
"                           Force the default duration of a track to X.\n"
"                           X can be a floating point number or a fraction.\n"
msgstr ""
"  --default-duration <TID:Xs|ms|us|ns|fps>\n"
"                           Tvingar standardvaraktigheten för ett spår till X.\n"
"                           X kan vara ett flyttal eller en bråkdel.\n"

#: src/merge/mkvmerge.cpp:284
#, no-c-format
msgid " Options that only apply to audio tracks:\n"
msgstr " Alternativ som endast gäller för ljudspår:\n"

#: src/merge/mkvmerge.cpp:285
#, no-c-format
msgid ""
"  --aac-is-sbr <TID[:0|1]> The track with the ID is HE-AAC/AAC+/SBR-AAC\n"
"                           or not. The value ':1' can be omitted.\n"
msgstr ""
"  --aac-is-sbr <TID[:0|1]> Spåret med ID är HE-AAC/AAC+/SBR-AAC\n"
"                           eller inte. Värdet ':1' kan utelämnas.\n"

#: src/merge/mkvmerge.cpp:287
#, no-c-format
msgid ""
"  --reduce-to-core <TID>   Keeps only the core of audio tracks that support\n"
"                           HD extensions instead of copying both the core\n"
"                           and the extensions.\n"
msgstr ""
"  --reduce-to-core <TID> Behåller endast kärnan av ljudspår som stöder\n"
"                           HD-tillägg istället för att kopiera både kärnan\n"
"                           och tilläggen.\n"

#: src/merge/mkvmerge.cpp:290
#, no-c-format
msgid ""
"  --remove-dialog-normalization-gain <TID>\n"
"                           Removes or minimizes the dialog normalization gain\n"
"                           by modifying audio frame headers.\n"
msgstr ""
"  --remove-dialog-normalization-gain <TID>\n"
"                           Tar bort eller minimerar dialognormaliseringsförstärkningen\n"
"                           genom att ändra ljudramens huvuden.\n"

#: src/merge/mkvmerge.cpp:293
#, no-c-format
msgid ""
"  --audio-emphasis <TID:n|keyword>\n"
"                           Sets the audio emphasis track header value. It can\n"
"                           be either a number or a keyword (see\n"
"                           '--list-audio-emphasis' for the full list).\n"
msgstr ""
"  --audio-emphasis <TID:n|nyckelord>\n"
"                           Ställer in spårhuvudvärdet för ljudets betoning. Det kan\n"
"                           vara antingen ett nummer eller ett nyckelord (se\n"
"                           \"--list-audio-emphasis\" för en fullständig lista).\n"

#: src/merge/mkvmerge.cpp:298
#, no-c-format
msgid " Options that only apply to video tracks:\n"
msgstr " Alternativ som endast gäller för videospår:\n"

#: src/merge/mkvmerge.cpp:299
#, no-c-format
msgid ""
"  --fix-bitstream-timing-information <TID[:bool]>\n"
"                           Adjust the frame/field rate stored in the video\n"
"                           bitstream to match the track's default duration.\n"
msgstr ""
"  --fix-bitstream-timing-information <TID[:bool]>\n"
"                           Justera den frame/field rate som lagras i videons\n"
"                           bitströmmen så att den matchar spårets standardlängd.\n"

#: src/merge/mkvmerge.cpp:302
#, no-c-format
msgid ""
"  -f, --fourcc <FOURCC>    Forces the FourCC to the specified value.\n"
"                           Works only for video tracks.\n"
msgstr ""
"  -f, --fourcc <FOURCC> Forcerar FourCC till det angivna värdet.\n"
"                           Fungerar endast för videospår.\n"

#: src/merge/mkvmerge.cpp:304
#, no-c-format
msgid ""
"  --aspect-ratio <TID:f|a/b>\n"
"                           Sets the display dimensions by calculating\n"
"                           width and height for this aspect ratio.\n"
msgstr ""
"  --aspect-ratio <TID:f|a/b>\n"
"                           Ställer in visningsdimensionerna genom att beräkna\n"
"                           bredd och höjd för detta bildförhållande.\n"

#: src/merge/mkvmerge.cpp:307
#, no-c-format
msgid ""
"  --aspect-ratio-factor <TID:f|a/b>\n"
"                           First calculates the aspect ratio by multi-\n"
"                           plying the video's original aspect ratio\n"
"                           with this factor and calculates the display\n"
"                           dimensions from this factor.\n"
msgstr ""
"  --aspect-ratio-faktor <TID:f|a/b>\n"
"                           Beräknar först bildförhållandet genom att multiplicera\n"
"                           genom att multiplicera videons ursprungliga bildförhållande\n"
"                           med denna faktor och beräknar visningsdimensionerna\n"
"                           dimensioner från denna faktor.\n"

#: src/merge/mkvmerge.cpp:312
#, no-c-format
msgid ""
"  --display-dimensions <TID:width>x<height>\n"
"                           Explicitly set the display dimensions.\n"
msgstr ""
"  --display-dimensions <TID:width>x<height>\n"
"                           Explicit inställning av visningsdimensionerna.\n"

#: src/merge/mkvmerge.cpp:314
#, no-c-format
msgid ""
"  --cropping <TID:left,top,right,bottom>\n"
"                           Sets the cropping parameters.\n"
msgstr ""
"  --cropping <TID:vänster,topp,höger,botten>\n"
"                           Ställer in parametrarna för beskärning.\n"

#: src/merge/mkvmerge.cpp:316
#, no-c-format
msgid ""
"  --field-order <TID:n>    Sets the video field order parameter\n"
"                           (see documentation for valid values).\n"
msgstr ""
"  --field-order <TID:n> Ställer in parametern för videofältordning\n"
"                           (se dokumentation för giltiga värden).\n"

#: src/merge/mkvmerge.cpp:318
#, no-c-format
msgid ""
"  --stereo-mode <TID:n|keyword>\n"
"                           Sets the stereo mode parameter. It can\n"
"                           either be a number 0 - 14 or a keyword\n"
"                           (use '--list-stereo-modes' to see the full list).\n"
msgstr ""
"  --stereo-mode <TID:n|nyckelord>\n"
"                           Ställer in parametern för stereoläget. Den kan\n"
"                           antingen vara ett nummer 0 - 14 eller ett nyckelord\n"
"                           (använd '--list-stereo-modes' för att se hela listan).\n"

#: src/merge/mkvmerge.cpp:322
#, no-c-format
msgid ""
"  --color-matrix-coefficients <TID:n>\n"
"                           Sets the matrix coefficients of the video used\n"
"                           to derive luma and chroma values from red, green\n"
"                           and blue color primaries.\n"
msgstr ""
"  --color-matrix-coefficients <TID:n>\n"
"                           Ställer in matriskoefficienterna för den video som används\n"
"                           för att härleda luma- och kromavärden från röda, gröna\n"
"                           och blå färgprimärvärden.\n"

#: src/merge/mkvmerge.cpp:326
#, no-c-format
msgid ""
"  --color-bits-per-channel <TID:n>\n"
"                           Sets the number of coded bits for a color \n"
"                           channel. A value of 0 indicates that the number is\n"
"                           unspecified.\n"
msgstr ""
"  --color-bits-per-channel <TID:n>\n"
"                           Ställer in antalet kodade bitar för en färg \n"
"                           kanal. Ett värde på 0 indikerar att antalet är\n"
"                           ospecificerat.\n"

#: src/merge/mkvmerge.cpp:330
#, no-c-format
msgid ""
"  --chroma-subsample <TID:hori,vert>\n"
"                           The amount of pixels to remove in the Cr and Cb\n"
"                           channels for every pixel not removed horizontally\n"
"                           and vertically.\n"
msgstr ""
"  --chroma-subsample <TID:hori,vert>\n"
"                           Antalet pixlar som ska tas bort i Cr- och Cb-kanalerna\n"
"                           kanaler för varje pixel som inte tas bort horisontellt\n"
"                           och vertikalt.\n"

#: src/merge/mkvmerge.cpp:334
#, no-c-format
msgid ""
"  --cb-subsample <TID:hori,vert>\n"
"                           The amount of pixels to remove in the Cb channel\n"
"                           for every pixel not removed horizontally and\n"
"                           vertically. This is additive with\n"
"                           --chroma-subsample.\n"
msgstr ""
"  --cb-subsample <TID:hori,vert>\n"
"                           Antalet pixlar som ska tas bort i Cb-kanalen\n"
"                           för varje pixel som inte tas bort horisontellt och\n"
"                           vertikalt. Detta är additivt med\n"
"                           --chroma-subsample.\n"

#: src/merge/mkvmerge.cpp:339
#, no-c-format
msgid ""
"  --chroma-siting <TID:hori,vert>\n"
"                            How chroma is sited horizontally/vertically.\n"
msgstr ""
"  --chroma-sittning <TID:hori,vert>\n"
"                            Hur chroma är placerad horisontellt/vertikalt.\n"

#: src/merge/mkvmerge.cpp:341
#, no-c-format
msgid "  --color-range <TID:n>   Clipping of the color ranges.\n"
msgstr "  --color-range <TID:n>   Klippning av färgområdena.\n"

#: src/merge/mkvmerge.cpp:342
#, no-c-format
msgid ""
"  --color-transfer-characteristics <TID:n>\n"
"                           The transfer characteristics of the video.\n"
msgstr ""
"  --color-transfer-characteristics <TID:n>\n"
"                           Överföringsegenskaperna för videon.\n"

#: src/merge/mkvmerge.cpp:344
#, no-c-format
msgid ""
"  --color-primaries <TID:n>\n"
"                           The color primaries of the video.\n"
msgstr ""
"  --color-primaries <TID:n>\n"
"                           Videons primära färger.\n"

#: src/merge/mkvmerge.cpp:346
#, no-c-format
msgid ""
"  --max-content-light <TID:n>\n"
"                           Maximum brightness of a single pixel in candelas\n"
"                           per square meter (cd/m²).\n"
msgstr ""
"  --max-content-light <TID:n>\n"
"                           Maximal ljusstyrka för en enskild pixel i candela\n"
"                           per kvadratmeter (cd/m²).\n"

#: src/merge/mkvmerge.cpp:349
#, no-c-format
msgid ""
"  --max-frame-light <TID:n>\n"
"                           Maximum frame-average light level in candelas per\n"
"                           square meter (cd/m²).\n"
msgstr ""
"  --max-frame-light <TID:n>\n"
"                           Maximal genomsnittlig ljusnivå för bildrutan i candela per\n"
"                           kvadratmeter (cd/m²).\n"

#: src/merge/mkvmerge.cpp:352
#, no-c-format
msgid ""
"  --chromaticity-coordinates <TID:red-x,red-y,green-x,green-y,blue-x,blue-y>\n"
"                           Red/Green/Blue chromaticity coordinates as defined\n"
"                           by CIE 1931.\n"
msgstr ""
"  --chromaticity-coordinates <TID:red-x,red-y,green-x,green-y,blue-x,blue-y>\n"
"                           Röd/Grön/Blå kromaticitetskoordinater enligt definitionen\n"
"                           av CIE 1931.\n"

#: src/merge/mkvmerge.cpp:355
#, no-c-format
msgid ""
"  --white-color-coordinates <TID:x,y>\n"
"                           White color chromaticity coordinates as defined\n"
"                           by CIE 1931.\n"
msgstr ""
"  --white-color-coordinates <TID:x,y>\n"
"                           Koordinater för kromaticitet för vit färg enligt definitionen\n"
"                           av CIE 1931.\n"

#: src/merge/mkvmerge.cpp:358
#, no-c-format
msgid ""
"  --max-luminance <TID:float>\n"
"                           Maximum luminance in candelas per square meter\n"
"                           (cd/m²).\n"
msgstr ""
"  --max-luminance <TID:float>\n"
"                           Maximal luminans i candela per kvadratmeter\n"
"                           (cd/m²).\n"

#: src/merge/mkvmerge.cpp:361
#, no-c-format
msgid ""
"  --min-luminance <TID:float>\n"
"                           Minimum luminance in candelas per square meter\n"
"                           (cd/m²).\n"
msgstr ""
"  --min-luminance <TID:float>\n"
"                           Minsta luminans i candela per kvadratmeter\n"
"                           (cd/m²).\n"

#: src/merge/mkvmerge.cpp:364
#, no-c-format
msgid ""
"  --projection-type <TID:method>\n"
"                           Projection method used (0–3).\n"
msgstr ""
"  --projection-type <TID:method>\n"
"                           Projiceringsmetod som används (0-3).\n"

#: src/merge/mkvmerge.cpp:366
#, no-c-format
msgid ""
"  --projection-private <TID:data>\n"
"                           Private data that only applies to a specific\n"
"                           projection (as hex digits).\n"
msgstr ""
"  --projection-private <TID:data>\n"
